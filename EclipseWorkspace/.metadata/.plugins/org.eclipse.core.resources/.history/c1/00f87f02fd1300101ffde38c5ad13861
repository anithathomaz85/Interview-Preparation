package gts;

public class Q22_DuplicateNumberInLinkedList {
	
	Node head;
	
	public static void main(String[] args) {
		NewLinkedList newLinedList = new NewLinkedList();
		
		newLinedList.addNode(12);
		newLinedList.addNode(12);
		newLinedList.addNode(14);
		newLinedList.addNode(15);
		newLinedList.addNode(14);
		newLinedList.addNode(19);
		
		newLinedList.findDuplicates();
	}
	
	public void addNode(int value) {
		Node newNode = new Node(value);
		
		if(head == null) {
			head = newNode;
		}else {
			Node temp = head;
			while(temp.next != null) {
				temp = temp.next;
			}
			temp.next = newNode;
		}
	}

}
