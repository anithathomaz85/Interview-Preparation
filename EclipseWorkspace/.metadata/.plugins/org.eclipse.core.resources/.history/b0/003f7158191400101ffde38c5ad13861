package gts;

public class Node {
	int data;
	Node next;
	
	Node(int x){
		data=x;	
		this.next = null;
	}
	

    Node head;
    
	 private void addNode(int value) {
	        Node newNode = new Node(value);

	        if (head == null) {
	            head = newNode;
	        } else {
	            Node temp = head;
	            while (temp.next != null) {
	                temp = temp.next;
	            }
	            temp.next = newNode;
	        }
	    }

}

