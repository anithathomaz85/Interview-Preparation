package gts;

public class Q34_BinaryArrayCountAndSort {

}

public static int[] sortBinaryArray(int[] arr) {
	 
	int zeroCount = 0;

	for (int num : arr) {
		if (num == 0)
			zeroCount++;
	}

	for (int i = 0; i < arr.length; i++) { 

		if (i < zeroCount) {

			arr[i] = 0;
		} else {
			arr[i] = 1;
		}
	}
	return arr;
}

