package gts;

public class B_FibonacciSeries {
	public static void main(String[] args) {
		
		attempt1();
	
	}

	private static void attempt1() {
		 int num = 6;
		 int n1=0,n2=1,n3;
		 
		 System.out.println(n1+" "+n2);
		 for(int i=2;i<=6;i++) {
			 n3=n1+n2;
			 n1=n2;
			 n2=n3;
			 System.out.print(" "+n3);
		 }
		
	}
}
