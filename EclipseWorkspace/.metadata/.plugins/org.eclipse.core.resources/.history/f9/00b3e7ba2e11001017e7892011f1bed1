package gts;

import org.junit.jupiter.api.Test;

public class A_MoveZeroesToEndTest {
	
	private A_MoveZerosToEnd moveZerosToEnd;
	
	A_MoveZeroesToEndTest(){
		moveZerosToEnd = new A_MoveZerosToEnd();
	}
	
	@Test
	void testA_MoveZeroesToEnd1() {
		int[] arr = {0, 1, 0, 3, 12};
	    int[] expectedResult = {1, 3, 12, 0, 0};
	    
	    moveZerosToEnd.moveZerosToEnd(arr);
	    assertArrayEquals(expectedResult,arr);
	}
	

}
